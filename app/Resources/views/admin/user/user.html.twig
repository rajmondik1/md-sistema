{% block body %}
<body>
<h1>Vartotojai</h1>
    <table id="table" class="table table-striped table-hover table-condensed">
    <thead>
        <tr>
            <th>Id</th>
            <th>{{ 'form.username'|trans({}, 'FOSUserBundle') }}</th>
            <th>{{ 'form.email'|trans({}, 'FOSUserBundle') }}</th>
            <th>Paskutinis prisijungimas</th>
        </tr>
    </thead>
    <tbody>
{% for user in users %}
        <tr>
            <td>{{ user.id|e }}</td>
            <td>{{ user.username|e }}</td>
            <td>{{ user.email|e }}</td>
            <td>{{ user.lastLogin is empty ? "" : user.lastLogin|date("Y:m:d") }}</td>
            <td> <a class="btn btn-danger" href="{{ path('admin_delete', {user: user.id}) }}">Istrinti</a></td>
        </tr>
{% endfor %}
    </tbody>
    </table>
</body>
{% endblock %}
