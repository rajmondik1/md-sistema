
{% block body %}

<h1>Programa</h1>
    <table id="table" class="table table-striped table-hover table-condensed">
    <thead>
        <tr>
            <th>Id</th>
            <th>Pavadinimas</th>

        </tr>
    </thead>
    <tbody>
    <h3><a href="{{ path('programa_add') }}">prideti programa</a></h3>

    {% for programa in programa %}
        <tr>
            <td>{{ programa.id|e }}</td>
            <td>{{ programa.pavadinimas }}</td>
            <td><a href="{{ path("programa_delete", {programa : programa.id}) }}">delete</a></td>
        </tr>
{% endfor %}
    </tbody>
    </table>

{% endblock %}